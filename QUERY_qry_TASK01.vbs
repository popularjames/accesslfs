SELECT ShowAuditRequestByName.AuditId, ShowAuditRequestByName.TaskId, ShowAuditRequestByName.TaskName, ShowAuditRequestByName.TaskDescription AS TaskDesc, Replace([Requestor],"ALEX","Alex") AS Requestor2, Replace((IIf([AssignedTo]="","--",[AssignedTo])),"ALEX","Alex") AS AssignedTo2, ShowAuditRequestByName.TaskWeight, Format([RequestedDate],"yyyy-mm-dd") AS RequestDt, Format([RequestedDeliveryDate],"yyyy-mm-dd") AS RequestedDeliveryDt, Format([ProjectedDeliveryDate],"yyyy-mm-dd") AS ProjectedDeliveryDt, Format([ActualDeliveryDate],"yyyy-mm-dd") AS ActualDeliveryDt, Format([CommentDate],"yyyy-mm-dd") AS CommentDt, Replace([CommentBy],"ALEX","Alex") AS CommentBy2, ShowAuditRequestByName.TaskNotes AS Comment, ShowAuditRequestByName.Private, ShowAuditRequestByName.AuditDesc
FROM ShowAuditRequestByName
WHERE (((Format([RequestedDate],"yyyy-mm-dd"))>="2012-01-13") AND ((ShowAuditRequestByName.AuditDesc) Like "*medicare*"));
